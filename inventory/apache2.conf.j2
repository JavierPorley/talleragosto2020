<Proxy balancer://mycluster>
    # Define back-end servers:

    # Server 1
    BalancerMember "{{ server1 }}"

    # Server 2
    BalancerMember "{{ server2 }}"
</Proxy>

<VirtualHost *:80>
    ProxyPreserveHost On

    # Servers to proxy the connection, or;
    # List of application servers:
    # Usage:
    # ProxyPass / http://192.168.0.194:80/
    # ProxyPassReverse / http://192.168.0.194:80/
    # Example:
    ProxyPass / "{{ server1 }}"
    ProxyPassReverse / "{{ server2 }}"

    #ServerName localhost
</VirtualHost>
